{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "predeploy": "npm install; npm run build",
    "public": "dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  }
}
